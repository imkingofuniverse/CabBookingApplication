Control Layer(controller) :

getAllTrips(int customerId) :
url : "http://localhost:9091/AdminController/allTrips"

getTripsCabwise() :
url : "http://localhost:9091/AdminController/allTripsCab"

getTripsCustomerwise() : 
url : "http://localhost:9091/AdminController/allTripsCustomer"

getTripsDatewise() :
url : "http://localhost:9091/AdminController/allTripsDate"

getAllTripsForDays(int customerId,LocalDateTime fromDate,LocalDateTime toDate) :
url : "http://localhost:9091/AdminController/allTripsDays"

